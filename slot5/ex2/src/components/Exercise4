export function Exercise4() {
    const ages = [33, 12, 20, 16];
    const [first, , third = 0, ...restAges] = ages;

    return (
        <div>   
            <h2>Exercise 4: Destructuring Array</h2>
            <p>First: {first}</p>
            <p>Third: {third}</p>
            <p>Rest Ages: {JSON.stringify(restAges)}</p>
            <p>hello</p>
        </div>
    );
}

export default Exercise4;